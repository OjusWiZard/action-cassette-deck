{
  "name": "action-cassette-deck",
  "version": "1.1.0",
  "private": true,
  "packageManager": "pnpm@8.0.0",
  "description": "Action to manage the synchronization between the vcr cassette repo and main rotki repo workflows",
  "keywords": [
    "actions",
    "vcr",
    "cassette"
  ],
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rotki/action-cassette-deck.git"
  },
  "author": "Rotki Solutions GmbH <info@rotki.com>",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "all": "pnpm run build && pnpm run lint && pnpm run package && pnpm test",
    "release": "bumpp --no-push",
    "prepare": "husky install"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/github": "5.1.1",
    "@octokit/webhooks-types": "7.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@rotki/eslint-config": "1.1.1",
    "@types/node": "20.9.2",
    "@vercel/ncc": "0.36.1",
    "bumpp": "9.2.0",
    "eslint": "8.48.0",
    "eslint-plugin-github": "4.9.2",
    "husky": "8.0.3",
    "js-yaml": "4.1.0",
    "lint-staged": "14.0.1",
    "nock": "13.3.3",
    "typescript": "5.1.6",
    "vitest": "0.34.3"
  },
  "engines": {
    "node": ">=20 <21",
    "pnpm": ">=8 <9"
  },
  "lint-staged": {
    "*.{js,ts,vue,yml,json}": "eslint"
  }
}
